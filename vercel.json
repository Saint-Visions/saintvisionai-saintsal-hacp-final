{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "server/api/chat.ts": {
      "runtime": "nodejs18.x"
    },
    "server/api/health.ts": {
      "runtime": "nodejs18.x"
    },
    "server/api/stripe.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
