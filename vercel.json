{
  "version": 2,
  "functions": {
    "server/api/chat.ts": {
      "runtime": "nodejs18.x"
    },
    "server/api/health.ts": {
      "runtime": "nodejs18.x"
    },
    "server/api/stripe.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/api/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}

